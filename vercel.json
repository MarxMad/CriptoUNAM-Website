{
  "buildCommand": "cd criptounam && npm install && npm run build",
  "outputDirectory": "criptounam/dist",
  "installCommand": "cd criptounam && npm install",
  "devCommand": "cd criptounam && npm run dev",
  "rewrites": [
    {
      "source": "/((?!api/.*|images/.*|assets/.*).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
